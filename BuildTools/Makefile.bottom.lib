	$(MAKE) -$(MAKEFLAGS) LinuxTcov.Static

# All W64 targets
#
W64:
	$(MAKE) -$(MAKEFLAGS) W64Debug.Pic
	$(MAKE) -$(MAKEFLAGS) W64Debug.Static
