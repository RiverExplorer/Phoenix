# ALL TARGETS
#
all :
	$(MAKE) -$(MAKEFLAGS) debug.Pic
	$(MAKE) -$(MAKEFLAGS) debug.Static
	$(MAKE) -$(MAKEFLAGS) release.Pic
	$(MAKE) -$(MAKEFLAGS) release.Static
	$(MAKE) -$(MAKEFLAGS) tcov.Pic
	$(MAKE) -$(MAKEFLAGS) tcov.Static

# All DEBUG targets.
#
debug Debug:
	$(MAKE) -$(MAKEFLAGS) LinuxDebug.Pic
	$(MAKE) -$(MAKEFLAGS) LinuxDebug.Static
	$(MAKE) -$(MAKEFLAGS) W64Debug.Pic
	$(MAKE) -$(MAKEFLAGS) W64Debug.Static
	$(MAKE) -$(MAKEFLAGS) AndroidDebug.Pic
	$(MAKE) -$(MAKEFLAGS) AndroidDebug.Static

# All RELEASE targets
#
release Release:
	$(MAKE) -$(MAKEFLAGS) LinuxRelease.Pic
	$(MAKE) -$(MAKEFLAGS) LinuxRelease.Static
	$(MAKE) -$(MAKEFLAGS) W64Release.Pic
	$(MAKE) -$(MAKEFLAGS) W64Release.Static
	$(MAKE) -$(MAKEFLAGS) AndroidRelease.Pic
	$(MAKE) -$(MAKEFLAGS) AndroidRelease.Static

# All TCOV targets
#
tcov Tcov:
	$(MAKE) -$(MAKEFLAGS) LinuxTcov.Pic
	$(MAKE) -$(MAKEFLAGS) LinuxTcov.Static
	$(MAKE) -$(MAKEFLAGS) W64Tcov.Pic
	$(MAKE) -$(MAKEFLAGS) W64Tcov.Static
	$(MAKE) -$(MAKEFLAGS) AndroidTcov.Pic
	$(MAKE) -$(MAKEFLAGS) AndroidTcov.Static

# All LINUX targets
#
Linux:
	$(MAKE) -$(MAKEFLAGS) LinuxDebug.Pic
	$(MAKE) -$(MAKEFLAGS) LinuxDebug.Static
	$(MAKE) -$(MAKEFLAGS) LinuxRelease.Pic
	$(MAKE) -$(MAKEFLAGS) LinuxRelease.Static
