LinuxDebug : LinuxDebug.Pic LinuxDebug.Static TestSubDirs

LinuxDebug.Pic : LocalNeeds Dirs LinuxDebugIncludes PreBuildDirs $(LINUX_TARGET.Debug.Pic) PostBuild_LinuxDebug SubDirs 

LinuxDebug.Static : LocalNeeds Dirs LinuxDebugIncludes PreBuildDirs $(LINUX_TARGET.Debug.Static) PostBuild_LinuxDebug SubDirs

$(LINUX_TARGET.Debug.Pic) : $(XDR_GENERATED.all) $(LINUX_OBJECTS.Debug.Pic)
	$(LINUX_CXX) -shared $(LINUX_LDFLAGS.Debug.Pic) -o $(LINUX_TARGET.Debug.Pic) $(LINUX_OBJECTS.Debug.Pic) $(LIBS) $(LINUX_LIBS)
	$(INSTALL.RX) $(LINUX_TARGET.Debug.Pic) $(LINUX_INSTALL_DIR.Debug)/lib
	(cd $(LINUX_TARGET.Debug) ; $(RM) lib$(TARGET).so ; ln -s lib$(TARGET).so.$(VERSION) lib$(TARGET).so)
	(cd $(LINUX_INSTALL_DIR.Debug)/lib ; $(RM) lib$(TARGET).so ; ln -s lib$(TARGET).so.$(VERSION) lib$(TARGET).so)

$(LINUX_TARGET.Debug.Static) : $(XDR_GENERATED.all) $(LINUX_OBJECTS.Debug.Static)
	$(AR) rv $(LINUX_TARGET.Debug.Static) $(LINUX_OBJECTS.Debug.Static)
	$(INSTALL.RO) $(LINUX_TARGET.Debug.Static) $(LINUX_INSTALL_DIR.Debug)/lib

LinuxRelease : LinuxRelease.Pic LinuxRelease.Static TestSubDirs

LinuxRelease.Pic : LocalNeeds Dirs LinuxReleaseIncludes PreBuildDirs $(LINUX_TARGET.Release.Pic) PostBuild_LinuxRelease SubDirs

