	$(MAKE) -$(MAKEFLAGS) W64Tcov.Static

# All ANDROID targets
#
Android:
	$(MAKE) -$(MAKEFLAGS) AndroidDebug.Pic
	$(MAKE) -$(MAKEFLAGS) AndroidDebug.Static
	$(MAKE) -$(MAKEFLAGS) AndroidRelease.Pic
	$(MAKE) -$(MAKEFLAGS) AndroidRelease.Static
	$(MAKE) -$(MAKEFLAGS) AndroidTcov.Pic
