W64Debug.Static : LocalNeeds Dirs W64DebugIncludes  PreBuildDirs $(W64_TARGET.Debug.Static) PostBuild_W64Debug SubDirs
endif

$(W64_TARGET.Debug.Pic) : $(XDR_GENERATED.all) $(W64_OBJECTS.Debug.Pic)
	$(W64_CXX) -shared -fPIC $(W64_LDFLAGS.Debug.Pic) -o $(W64_TARGET.Debug.Pic) $(W64_OBJECTS.Debug.Pic) $(LIBS) $(W64_LIBS)
	if [ -r $(W64_TARGET.Debug.Pic) ]; then install $(W64_TARGET.Debug.Pic) $(W64_INSTALL_DIR.Debug)/lib; fi;
ifneq ($(W64_SOURCES.cs),)
	if [ -r $(W64_TARGET_CS.Debug).dll ]; then install $(W64_TARGET_CS.Debug).dll $(W64_INSTALL_DIR.Debug)/lib fi;
endif

$(W64_TARGET.Debug.Static) : $(XDR_GENERATED.all) $(W64_OBJECTS.Debug.Static)
	$(AR) rv $(W64_TARGET.Debug.Static) $(W64_OBJECTS.Debug.Static)
	if [ -r $(W64_TARGET.Debug.Static) ]; then install $(W64_TARGET.Debug.Static) $(W64_INSTALL_DIR.Debug)/lib; fi;

